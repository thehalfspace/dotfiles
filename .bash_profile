# terminal default line = current directory
export PS1="\e[0;31m[\W] \$ \e[m "

# activate colors in terminal
export CLICOLOR=1

# change the colors of terminal
export LSCOLORS=gxfxcxdxbxegedabagaced

# use sublime text from terminal
alias sublime="open -a /Applications/Sublime\ Text.app"

# added by Miniconda3 4.3.21 installer
export PATH="/Users/prithvithakur/miniconda3/bin:$PATH"

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm" # Load RVM into a shell session *as a function*

